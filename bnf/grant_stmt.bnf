grant_stmt ::=
	'GRANT' 'ALL' 'PRIVILEGES' 'ON' targets 'TO' name_list
	| 'GRANT' 'ALL'  'ON' targets 'TO' name_list
	| 'GRANT' privilege_list 'ON' targets 'TO' name_list
	| 'GRANT' privilege_list 'TO' name_list
	| 'GRANT' privilege_list 'TO' name_list 'WITH' 'ADMIN' 'OPTION'
	| 'GRANT' 'ALL' 'PRIVILEGES' 'ON' 'TYPE' target_types 'TO' name_list
	| 'GRANT' 'ALL'  'ON' 'TYPE' target_types 'TO' name_list
	| 'GRANT' privilege_list 'ON' 'TYPE' target_types 'TO' name_list
	| 'GRANT' 'ALL' 'PRIVILEGES' 'ON' 'SCHEMA' schema_name_list 'TO' name_list
	| 'GRANT' 'ALL'  'ON' 'SCHEMA' schema_name_list 'TO' name_list
	| 'GRANT' privilege_list 'ON' 'SCHEMA' schema_name_list 'TO' name_list
	| 'GRANT' 'ALL' 'PRIVILEGES' 'ON' 'ALL' 'TABLES' 'IN' 'SCHEMA' schema_name_list 'TO' name_list
	| 'GRANT' 'ALL'  'ON' 'ALL' 'TABLES' 'IN' 'SCHEMA' schema_name_list 'TO' name_list
	| 'GRANT' privilege_list 'ON' 'ALL' 'TABLES' 'IN' 'SCHEMA' schema_name_list 'TO' name_list
